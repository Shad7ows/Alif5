س = 9 + 8 - 7 * 6 / 5 ^ /^ 4

لاجل ص في (5, 7, 9):
	اذا ص == 9:
		توقف
	اطبع(ص)

بينما س < 30:
	اذا س == 20.32:
		س += 5
		استمر
	اطبع(س)
	س += 5
	

اذا س == 5:
	اطبع("س تساوي : 5")
اواذا س == 15.32:
	اطبع(م"س = 9 + 8 - 7 * 6 / 5 ^ /^ 4 = { س }")
والا:
	اطبع(م"يبدو أن س = { س }")








